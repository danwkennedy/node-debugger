{
  "manifest_version": 2,
  "name": "Node Debugger",
  "description": "Debug Node in Chrome devtools",
  "version": "0.0.1",
  "browser_action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "http://*/",
    "storage"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
